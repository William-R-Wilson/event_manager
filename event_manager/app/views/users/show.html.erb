<h1><%= @user.name %></h1>
<p><%= @user.tagline %></p>
<% if logged_in? %>
  <h3>Events:</h3>
  <ol><%= render @event %></ol>
  <p><%= link_to "Add new event", new_event_path %></p>
  <p><%= link_to "Log Out", logout_path, method: "delete" %></p>
  

<% else %>
  <p><%= link_to "Log In", login_path %></p> 
  <% end %>

<%= link_to "new user", new_user_path %> | <%= link_to "all users", users_path %>